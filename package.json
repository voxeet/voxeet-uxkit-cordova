{
  "name": "cordova-plugin-voxeet",
  "version": "1.5.7",
  "description": "Cordova Voxeet Plugin",
  "cordova": {
    "id": "cordova-plugin-voxeet",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voxeet/voxeet-uxkit-cordova"
  },
  "bugs": {
    "url": "https://github.com/voxeet/voxeet-uxkit-cordova/issues"
  },
  "keywords": [
    "ecosystem:cordova",
    "cordova",
    "voxeet",
    "cordova-ios",
    "cordova-android",
    "dolby"
